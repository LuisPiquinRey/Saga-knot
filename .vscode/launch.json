{
    "configurations": [
        {
            "type": "java",
            "name": "Spring Boot-ConfigsApplication<configs>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.luispiquinrey.configs.ConfigsApplication",
            "projectName": "configs",
            "args": "",
            "envFile": "${workspaceFolder}/.env",
            "env": {
                "GITHUB_TOKEN": "${env:GITHUB_TOKEN}",
                "KEY_ENCRYPT": "${env:KEY_ENCRYPT}",
                "CREDENTIALS_SQL":"${env:CREDENTIALS_SQL}"
            },
            "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=52538 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=configs"
        },
        {
            "type": "java",
            "name": "Spring Boot-ProductApplication<product>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.luispiquinrey.product.ProductApplication",
            "projectName": "product",
            "args": "",
            "envFile": "${workspaceFolder}/.env",
            "env": {
                "CREDENTIALS_AXION": "${env:CREDENTIALS_AXION}"
            }
        },
        {
            "type": "java",
            "name": "Spring Boot-DiscoveryApplication<discovery>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.luispiquinrey.discovery.DiscoveryApplication",
            "projectName": "discovery",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "type": "java",
            "name": "Spring Boot-AdministrationApplication<administration>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.luispiquinrey.administration.AdministrationApplication",
            "projectName": "administration",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
